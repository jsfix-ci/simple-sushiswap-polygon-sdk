  "bugs": {
    "url": "https://github.com/tushargargght/simple-sushiswap-polygon-sdk/issues"
  },
  "homepage": "https://github.com/tushargargght/simple-sushiswap-polygon-sdk#readme",
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md",
    "LICENSE"
  ]
}
